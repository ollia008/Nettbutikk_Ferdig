@model Nettbutikk.LoginnModel

@{


}

@using (Html.BeginForm())
{

    <h1>login</h1>
    <form method="post">

        <p>
            <span>Brukernavn<label>*</label></span>
            <br />
            @Html.EditorFor(LoginnModel => LoginnModel.Username, new { @htmlattributes = new { @class = "control-form" } })
        </p>

        <p>
            <span>Passord<label>*</label></span>
            <br />
            @Html.EditorFor(LoginnModel => LoginnModel.Password, new { @htmlattributes = new { @class = "control-form" } })
        </p>

        <p>
            <input type="submit" value="submit" />
        </p>

    </form>
}